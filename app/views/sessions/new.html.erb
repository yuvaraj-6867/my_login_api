<h1>Sign In</h1>

<%= form_with url: signin_path, method: :post do |form| %>
  <div>
    <%= form.label :email %><br>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.label :password %><br>
    <%= form.password_field :password %>
  </div>

  <div>
    <%= form.submit "Sign In" %>
  </div>
<% end %>
